{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run copy-env && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "copy-env": "cp .env.example .env",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "clear_jest": "jest --clearCache",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test-e2e jest --config ./test/jest-e2e.json --forceExit --maxWorkers=4",
    "typeorm": "ts-node --require ts-node/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "generate-migration": "npm run typeorm -- migration:generate -n",
    "run-migrations": "npm run typeorm migration:run",
    "revert-migrations": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.12.0",
    "@elrondnetwork/erdjs": "10.2.4",
    "@elrondnetwork/erdjs-network-providers": "^0.1.5",
    "@elrondnetwork/erdnest": "^0.1.8",
    "@golevelup/nestjs-rabbitmq": "2.2.0",
    "@jenyus-org/nestjs-graphql-utils": "^1.6.4",
    "@nestjs-query/query-graphql": "0.30.0",
    "@nestjs/common": "8.4.4",
    "@nestjs/config": "2.0.0",
    "@nestjs/core": "8.4.4",
    "@nestjs/graphql": "9.1.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "8.4.4",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.4.4",
    "@nestjs/schedule": "1.1.0",
    "@nestjs/typeorm": "8.0.3",
    "@types/cache-manager": "^3.4.0",
    "agentkeepalive": "^4.1.4",
    "amqp-connection-manager": "^3.7.0",
    "amqplib": "^0.8.0",
    "apollo-server-express": "^2.25.2",
    "aws-sdk": "^2.974.0",
    "axios": "^0.21.4",
    "axios-retry": "^3.1.8",
    "bignumber.js": "^9.0.1",
    "cache-manager": "^3.6.1",
    "cache-manager-redis-store": "^2.0.0",
    "clarifai-nodejs-grpc": "^8.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "config": "^3.3.6",
    "dataloader": "^2.0.0",
    "errorhandler": "^1.5.1",
    "form-data": "^4.0.0",
    "graphql": "^15.5.0",
    "graphql-relay": "^0.7.0",
    "graphql-tools": "^8.2.0",
    "graphql-upload": "^12.0.0",
    "helmet": "^4.4.1",
    "jwks-rsa": "^2.0.3",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "nest-winston": "^1.6.2",
    "nestjs-redis": "https://github.com/ElrondNetwork/nestjs-redis#master",
    "object-hash": "^2.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prom-client": "^13.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4",
    "save": "^2.5.0",
    "swagger-ui-express": "^4.3.0",
    "tiny-async-pool": "^1.2.0",
    "typeorm": "^0.2.41",
    "winston": "3.7.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "8.4.4",
    "@types/express": "^4.17.11",
    "@types/graphql-relay": "^0.6.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.14.36",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@types/tiny-async-pool": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^27.5.1",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "ts-node": "10.7.0",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "resolutions": {
    "fs-capacitor": "6.2.0",
    "graphql-upload": "11.0.0"
  }
}
